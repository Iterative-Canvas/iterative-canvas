{
  "name": "iterative-canvas",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.26.0",
  "engines": {
    "node": "^24"
  },
  "scripts": {
    "dev": "concurrently \"pnpm run dev:next\" \"pnpm run dev:convex\"",
    "dev:next": "next dev --turbopack",
    "dev:convex": "convex dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "next typegen && tsc --noEmit",
    "test": "vitest",
    "shadcn": "sh -c 'npx shadcn@latest add \"$@\"' _",
    "format": "prettier . --write",
    "print-repo": "tree -I \"node_modules|.git|.next|.cursor|.github|.devcontainer\""
  },
  "dependencies": {
    "@ai-sdk/gateway": "^2.0.21",
    "@ai-sdk/react": "^2.0.23",
    "@auth/core": "^0.37.0",
    "@convex-dev/auth": "^0.0.90",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "ai": "^5.0.23",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "convex": "^1.31.0",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-resizable-panels": "^3.0.4",
    "streamdown": "^1.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5",
    "vitest": "^4.0.15"
  },
  "_notes": {
    "scripts.shadcn": "Adds shadcn/ui components to the project. Usage: `npm run shadcn -- form button card`"
  }
}
